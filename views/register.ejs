  <body class="text-center">
    
    <main class="form-signin">
      <form action="/user/register" method="POST">
        <img class="mb-4" src="https://cdn-images-1.medium.com/max/800/1*Wcf8lbU7KxjrQ0hggan-rg.png" alt="" width="150" height="120">
        <h1 class="h3 mb-3 fw-normal">register</h1>
        
        <% if(errors != null && errors.length > 0) { %>
          <% for (let i = 0; i < errors.length; i++) { %>
              <p class="alert alert-danger">
                  <%= errors[i] %>
              </p>
          <% } %>
      <% } %>
      <div class="form-floating">
        <input type="text" name="username" value="<%= username %>"class="form-control" id="username" >
        <label for="floatingPassword">username</label>
      </div>
        <div class="form-floating">
          <input type="password" name="password" value="<%= password %>"class="form-control" id="password" >
          <label for="floatingPassword">Password</label>
        </div>
        <div class="form-floating">
          <input type="text" name="firstname" value="<%= firstname %>"class="form-control" id="firstname" >
          <label for="floatingPassword">firstname</label>
        </div>
        <div class="form-floating">
          <input type="text" name="lastname" value="<%= lastname %>"class="form-control" id="lastname" >
          <label for="floatingPassword">lastname</label>
        </div>
        <div class="form-floating">
          <input type="email" name="email" class="form-control" value="<%= email %>" id="email" >
          <label for="floatingInput">Email</label>
        </div>
        <div class="form-floating">
          <input type="tel" name="phone" value="<%= phone %>"class="form-control" id="phone" >
          <label for="floatingPassword">phone</label>
        </div><br>
        <button class="w-100 btn btn-lg btn-dark" type="submit">สมัครสมาชิก</button>
        <p class="mt-3 mb-3 text-muted"><a href="/login">ล็อคอิน</a></p>
      </form>
    </main>   
 
  </body>
